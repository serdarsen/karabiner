{
  "title": "Desktop Shortcuts",
  "rules": [
    {
      "description": "Control–Command–Up Arrow to Switch to Desktop 1",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "up_arrow",
            "modifiers": {
              "mandatory": [
                "left_control",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "1",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Control–Command–Down Arrow to Switch to Desktop 3",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "down_arrow",
            "modifiers": {
              "mandatory": [
                "left_control",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "3",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Control–Command–Left Arrow to Control–Left Arrow",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_arrow",
            "modifiers": {
              "mandatory": [
                "left_control",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Control–Command–Right Arrow to Control–Right Arrow",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_arrow",
            "modifiers": {
              "mandatory": [
                "left_control",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    }
  ]
}